<component-set>
    <components>
        <component>
            <role-hint>inugami-maven-lifecycle_info</role-hint>
            <role>org.apache.maven.lifecycle.Lifecycle</role>
            <implementation>org.apache.maven.lifecycle.Lifecycle</implementation>
            <configuration>
                <id>inugami-maven-info</id>
                <phases>
                    <phase>validate</phase>
                    <phase>initialize</phase>
                    <phase>info</phase>
                </phases>

                <default-phases>
                    <deploy-plugin>io.inugami.maven.plugin.analysis:inugami-project-analysis-maven-plugin:handlerInformation
                    </deploy-plugin>
                </default-phases>
            </configuration>
        </component>

        <component>
            <role-hint>inugami-maven-lifecycle_analyse</role-hint>
            <role>org.apache.maven.lifecycle.Lifecycle</role>
            <implementation>org.apache.maven.lifecycle.Lifecycle</implementation>
            <configuration>
                <id>inugami-maven-analyze</id>
                <phases>
                    <phase>validate</phase>
                    <phase>initialize</phase>
                    <phase>generate-source</phase>
                    <phase>process-source</phase>
                    <phase>generate-resources</phase>
                    <phase>process-resources</phase>
                    <phase>compile</phase>
                    <phase>process-test-resources</phase>
                    <phase>test-compile</phase>
                    <phase>analyze</phase>
                </phases>

                <default-phases>
                    <deploy-plugin>io.inugami.maven.plugin.analysis:inugami-project-analysis-maven-plugin:handlerAnalyse
                    </deploy-plugin>
                </default-phases>
            </configuration>
        </component>
    </components>
</component-set>
